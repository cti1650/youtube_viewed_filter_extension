{
  "manifest_version": 3,
  "name": "Youtube Viewd Filter Extension",
  "version": "0.0.19",
  "description": "Extension to filter viewed videos on youtube",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "css": [
        "css/style.css"
      ],
      "js": [
        "js/script.js"
      ]
    }
  ],
  "action": {
    "default_title": "Youtube Viewd Filter Extension"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://www.youtube.com/"
  ]
}